#home
    view-box
        x-header 人员详情
        flexbox
            flexbox-item
                img(class="flex-demo2",src="")
            flexbox-item
                flexbox(orient="vertical")
                    flexbox-item
                        div(class="flex-demo") {{this.store.personInfo.eafName}} | {{checkSex(this.store.personInfo.cwrIdnum)}} | {{getAge(this.store.personInfo.cwrIdnum)}}岁
                    flexbox-item
                        div(class="flex-demo2") 查看证件
            flexbox-item
                img(class="flex-demo2",src="")
        cell(title='手机号码',
            :value = "this.store.personInfo.eafPhone",
            is-link =true)
            img(slot="icon",
                width="20",
                style="display:block;margin-right:5px;",
                src="data:image/png;base64,")
        cell(title='身份证号',
            :value = "this.store.personInfo.cwrIdnum",
            is-link =true)
            img(slot="icon",
                width="20",
                style="display:block;margin-right:5px;",
                src="data:image/png;base64,")
        cell(title='工种',
            :value = "this.store.personInfo.workType",
            is-link =true)
            img(slot="icon",
                width="20",
                style="display:block;margin-right:5px;",
                src="data:image/png;base64,")
        cell(title='状态',
            value = '在场',
            is-link =true)
            img(slot="icon",
                width="20",
                style="display:block;margin-right:5px;",
                src="data:image/png;base64,")
        cell(title='是否班组长',
            value = '是',
            is-link =true)
            img(slot="icon",
                width="20",
                style="display:block;margin-right:5px;",
                src="data:image/png;base64,")
        cell(title='评定等级',
            value = 'SSSSS',
            is-link =true)
            img(slot="icon",
                width="20",
                style="display:block;margin-right:5px;",
                src="data:image/png;base64,")
        swiper(auto height="200px",
            dots-position="center")
            swiper-item(v-for ="item in getProjectInfoList()")
                div(class="text-title") 参与项目
                flexbox
                    flexbox-item(span="1")
                        badge
                    flexbox-item
                        div(class="text-main") 项目名称
                div(class="text-gray") {{item.project_name}}
                flexbox
                    flexbox-item(span="1")
                        badge
                    flexbox-item(span="100")
                        div(class="text-main") 所在单位
                    flexbox-item
                        div(class="text-gray") {{item.project_license}}
                flexbox
                    flexbox-item(span="1")
                        badge
                    flexbox-item
                        div(class="text-main") 进场时间
                    flexbox-item
                        div(class="text-blue") {{getDateFormat(item.start_time)}}
                    flexbox-item(span="1")
                        badge
                    flexbox-item
                        div(class="text-main") 退场时间
                    flexbox-item
                        div(class="text-blue") {{getDateFormat(item.end_time)}}
        div
            div(class="text-title") 参与培训
            flexbox(orient="horizontal")
                flexbox-item(span="1")
                    img
                flexbox-item
                    flexbox(orient="horizontal")
                        flexbox(orient="vertical")
                            flexbox-item
                                div(class="text-main") 两制安全教育
                            flexbox-item
                                div(class="text-blue") 学习时长：4小时
                        flexbox(orient="vertical")
                            flexbox-item
                                div(class="text-main") 培训通过
                            flexbox-item
                                div(class="text-blue") 时间：2019-02-02
        div
            div(class="text-title") 出勤记录
            cell(title='本月出勤天数',
                value = '25天',
                is-link =true)
                img(slot="icon",
                    width="20",
                    style="display:block;margin-right:5px;",
                    src="data:image/png;base64,")
            cell(title='总出勤天数',
                value = '230天',
                is-link =true)
                img(slot="icon",
                    width="20",
                    style="display:block;margin-right:5px;",
                    src="data:image/png;base64,")
        div
            div(class="text-title") 工资明细
            cell(title='总收入',
                value = '￥10000',
                is-link =true)
                img(slot="icon",
                    width="20",
                    style="display:block;margin-right:5px;",
                    src="data:image/png;base64,")
            cell(title='总到账',
                value = '￥20000',
                is-link =true)
                img(slot="icon",
                    width="20",
                    style="display:block;margin-right:5px;",
                    src="data:image/png;base64,")
        div
            div(class="text-title") 员工评价
