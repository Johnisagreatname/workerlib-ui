                MenuItem(name="1-1")
                  .logo
                MenuItem(name="1-2")
                    Icon(type="md-home")
                    span 首页
                MenuItem(name="1-3")
                    Icon(type="ios-folder-open")
                    span 项目
                MenuItem(name="1-4")
                    Icon(type="md-people")
                    span 人员
                MenuItem(name="1-5")
                    Icon(type="ios-book")
                    span 培训
                MenuItem(name="1-6")
                    Icon(type="ios-build")
                    span 工种
                MenuItem(name="1-7")
